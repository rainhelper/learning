<div class="bnr_musicbar">
	<div class="viewFrame" style="width:100%; height:250px;">
		<div class="wrap_listRoll">
			#set($width = 728 * $upBannerList.size())
			<ul id="HighLightList" class="list_bnr list_view" style="width:$!{width}px;">
				#foreach($motTemplete in $upBannerList)
					#if( $StringUtils.isNotBlank("$motTemplete.imageUrl") )
						<li class="roll"><a href="$!{motTemplete.linkUrl}"><img src="$ImageUtil.getImageUrl("$!{motTemplete.imageUrl}", "728x250", "PHOTO", "C")" alt=""></a></li>
					#end
				#end
			</ul>
		</div>
	</div>
	<a id="msPrevBtn" class="btn_list btn_prev" href="javascript:;" title="이전 내용" onclick="test.moveToLeft();">이전 내용</a>
	<a id="msNextBtn" class="btn_list btn_next" href="javascript:;" title="다음 내용" onclick="test.moveToRight();">다음 내용</a>
</div>
