#macro(printTemplatePV $talkContent)
<div class="detail_cont detail_post">
    <h4 class="screen_out">소개 글 컨텐츠</h4>
	#if( "$!{talkContent.photoTitle}" != "")
        <strong class="tit_comm">$!{talkContent.photoTitle}</strong>
	#end
    <div class="visual_post" style="min-height:260px;">
        <strong class="screen_out">소개 글 이미지 목록</strong>
	    <div id="templetePV_$!{talkContent.templeteOrder}"></div>
    </div>
    <div class="info_post info_post_$!{talkContent.templeteOrder}">
        <p class="desc_post desc_post_$!{talkContent.templeteOrder}" id="desc_area" style="display:none; height:50px;">
            <em class="num_post_$!{talkContent.templeteOrder}">1.</em> <span class="desc_photo_text_$!{talkContent.templeteOrder}"></span>
        </p>
        <div class="paging_post" id="paging_area" style="z-index:888;">
				<span class="inner_paging">
					#foreach($photo in $talkContent.photoUrlList)
                        <span class="ico_comm num_page_$!{talkContent.templeteOrder}"  style="cursor: pointer;">${velocityCount}</span>
					#end
                </span>
        </div>
    </div>
</div>
	<script type="text/javascript">
        function resizePhotoViewer$!{talkEntireContent.templeteOrder}() {
            if (util.userAgent().platform != 'mobile') {
                var viewerWidth = jQuery("#templetePV_$!{talkEntireContent.templeteOrder}").width();
                var listHeight = viewerWidth * 3 / 4;
                var childSilde = jQuery("#templetePV_$!{talkEntireContent.templeteOrder} :first-child").attr('id');
                jQuery("#templetePV_$!{talkEntireContent.templeteOrder}").height(listHeight);
                jQuery("#" + childSilde).height(listHeight);
            }
        }

        var lastWidth = $(window).width();
        $(window).resize(function(){
            resizePhotoViewer$!{talkEntireContent.templeteOrder}();
            lastWidth = $(window).width();
        });

        $(window).load(function() {
            resizePhotoViewer$!{talkEntireContent.templeteOrder}();
        });
	</script>
#end

#macro(printTemplatePVData $talkContent)
    var items_$!{talkContent.templeteOrder} = [];
    #set( $count = 0 )
    #foreach($photo in $talkContent.photoUrlList)
        items_$!{talkContent.templeteOrder}.push({
            toHTML: function() { return '<a href="/talk/viewer?board_id=$!{talkContent.boardId}&temp_order=$!{talkContent.templeteOrder}&view_img_order=${count}" class="link_post">' +
	                                        '<img src="$!{ImageUtil.getFaceDetectImageUrl("$!{photo}", "C640x600")}" id="image_area" class="thumb_visual" alt="">' +
                                        '</a>';
                               }
        });
        #set( $count = $count + 1 )
    #end

    var items_text_${talkContent.templeteOrder} = [];
    #foreach($photo in $talkContent.photoList)
        items_text_$!{talkContent.templeteOrder}.push('$!{EscapeUtil.encodeHtmlTag($!{photo.imageComment})}');
    #end

    var dataSource_$!{talkContent.templeteOrder} = new slide.DataSource(items_$!{talkContent.templeteOrder});
    var option_$!{talkContent.templeteOrder} = {};
    var slider_$!{talkContent.templeteOrder} = new slide.Slide(jQuery("#templetePV_$!{talkContent.templeteOrder}").get(0), dataSource_$!{talkContent.templeteOrder}, option_$!{talkContent.templeteOrder});

    jQuery(".num_page_$!{talkContent.templeteOrder}").first().addClass("on");
        slider_$!{talkContent.templeteOrder}.on('slide:after', function _slideAfter(type) {
        jQuery(".num_page_$!{talkContent.templeteOrder}").removeClass("on");
        jQuery(".num_page_$!{talkContent.templeteOrder}").eq(dataSource_$!{talkContent.templeteOrder}.getIndex()).addClass("on");
    });

    jQuery(".num_page_${talkContent.templeteOrder}").click(function() {
        slider_${talkContent.templeteOrder}.setIndex(jQuery(this).index());
        jQuery(".num_page_${talkContent.templeteOrder}").removeClass("on");
        jQuery(".num_page_${talkContent.templeteOrder}").eq(dataSource_${talkContent.templeteOrder}.getIndex()).addClass("on");

        jQuery(".desc_photo_text_$!{talkContent.templeteOrder}").html(items_text_$!{talkContent.templeteOrder}[jQuery(this).index()]);
        jQuery(".num_post_$!{talkContent.templeteOrder}").html(jQuery(this).index() + 1 + ".");
    });

    var currentIndex_$!{talkContent.templeteOrder} = 0;
    slider_$!{talkContent.templeteOrder}.on('slide:after', function _slideAfter(type) {
        currentIndex_$!{talkContent.templeteOrder} = dataSource_$!{talkContent.templeteOrder}.getIndex();

        jQuery(".desc_photo_text_$!{talkContent.templeteOrder}").html(items_text_$!{talkContent.templeteOrder}[currentIndex_$!{talkContent.templeteOrder}]);
        jQuery(".num_post_$!{talkContent.templeteOrder}").html(currentIndex_$!{talkContent.templeteOrder} + 1 + ".");
    });

    if (items_text_$!{talkContent.templeteOrder}[0] != "") {
        jQuery(".desc_photo_text_$!{talkContent.templeteOrder}").html(items_text_$!{talkContent.templeteOrder}[currentIndex_$!{talkContent.templeteOrder}]);
        jQuery(".num_post_$!{talkContent.templeteOrder}").html(currentIndex_$!{talkContent.templeteOrder} + 1 + ".");
        jQuery(".desc_post_$!{talkContent.templeteOrder}").show();
    }else{
		jQuery(".info_post_$!{talkContent.templeteOrder}").addClass("info_type1");
	}

#end

