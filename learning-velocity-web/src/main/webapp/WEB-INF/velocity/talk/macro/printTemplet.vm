#macro(printTemplateBA $talkContent)
	<div class="detail_cont detail_post">
	    <h4 class="screen_out">소개 글 컨텐츠</h4>
	    #if( "$!{talkContent.title}" != "")
		    <strong class="tit_comm">$!{talkContent.title}</strong>
		#end

		#if($StringUtils.indexOf($!{talkContent.contents},"iframe") > 0)
			#set($new_contents = $!{StringUtils.replace($!{talkContent.contents},'src','class="iframeArea" src')})
            <p class="txt_comm">
	            $new_contents
            </p>
            <script type="text/javascript">
                daum.Music.Talk.resizeLivePlayer($(".iframeArea"));
            </script>
		#else
	    <p class="txt_comm">
		    $!{talkContent.contents}
	    </p>
		#end
	</div>
#end

#macro(printTemplateAI $talkContent)
	<div class="detail_cont detail_post">
	    <h4 class="screen_out">소개 글 컨텐츠</h4>
		#if( "$!{talkContent.title}" != "")
            <strong class="tit_comm">$!{talkContent.title}</strong>
		#end
        <div>
            <p class="txt_comm"></p>
            <strong class="screen_out">소개 글 이미지 목록</strong>
		    <a #if("$!{talkContent.linkUrl}" != "")href="$!{talkContent.linkUrl}"#end target="_blank" style="height: auto" class="link_post">
			    <img src="$!{ImageUtil.getMobileImageUrl("$!{talkContent.imageUrl}", "640x0", "PHOTO", "R")}" style="position: relative" class="thumb_visual" alt="$!{EscapeUtil.encodeHtmlTag($!{talkContent.title})}">
		    </a>
	    </div>
	</div>
#end

#macro(printTemplateAL $talkContent)
	<div class="detail_cont detail_music">
	    <h4 class="screen_out">음악 리스트 컨텐츠</h4>
		#if( "$!{talkContent.albumTitle}" != "")
            <strong class="tit_comm">$!{talkContent.albumTitle}</strong>
		#end
	    <div class="item_album">
	        <strong class="screen_out">앨범리스트</strong>
	        <ul class="list_album">
				#foreach($album in $talkContent.albumList)
	            <li>
	                <a #if("$!{talkContent.landingUrlList[$foreach.index]}" != "" && ($isAndroid == true || $isIos == true))href="javascript:daum.Music.Talk.landingKakaoMusicAlbum('$!{talkContent.landingUrlList[$foreach.index]}');"#end target="_blank" class="link_album">
	                     <img src="$!{ImageUtil.getMobileImageUrl("$!{album.mainImage.filePath}", "640x0", "PHOTO", "R")}" width="60" height="60" class="thumb_album" alt="$!{EscapeUtil.encodeHtmlTag($!{album.title})}">
						<span class="desc_album">
							<strong class="tit_music">$!{album.title}</strong>
							<span class="txt_name">$!{album.keyArtistList[0].name}</span>
						</span>
	                </a>
	            </li>
				#end
	        </ul>
	    </div>
	</div>
#end

#macro(printTemplateSL $talkContent)
	<div class="detail_cont detail_music">
	    <h4 class="screen_out">음악 리스트 컨텐츠</h4>
		#if( "$!{talkContent.songTitle}" != "")
	    <strong class="tit_comm">$!{talkContent.songTitle} </strong>
		#end
	    <div class="item_music">
	        <strong class="screen_out">음악리스트</strong>
            #set($trackIds = "")
	        <ol class="list_music">
				#foreach($song in $talkContent.songList)
	            <li>
                    #if($isAndroid == true || $isIos == true)
                        <a href="javascript:daum.Music.Talk.landingKakaoMusic('$!{talkContent.talkIdList[$foreach.index]}');" class="link_music">
                        #if("$!{talkContent.talkIdList[$foreach.index]}" != "0")
                            #set($trackIds = $trackIds + $!{talkContent.talkIdList[$foreach.index]} + "|")
                        #end
                    #else
                        <a href="javascript:;" class="link_music" style="cursor: default;">
                    #end
	                    <img src="$!{ImageUtil.getMobileImageUrl("$!{song.album.mainImage.filePath}", "640x0", "PHOTO", "R")}" width="38" height="38" class="thumb_music" alt="$!{EscapeUtil.encodeHtmlTag($!{song.title})}">
						<span class="info_music">
							<em class="num_music">#if( ${velocityCount} < 10 ) 0${velocityCount} #else ${velocityCount} #end </em>
							<span class="desc_music">
								<strong class="tit_music">$!{song.title}</strong>
								<span class="txt_name">$!{song.keyArtistList[0].name} - $!{song.album.title}</span>
                                #if($isAndroid == true || $isIos == true)
                                    <span class="ico_comm ico_music">음악 재생하기</span>
                                #end
							</span>
						</span>
	                </a>
	            </li>
				#end

	        </ol>
            #if("$!trackIds" != "" && $talkContent.songList.size() > 1)
                <div class="wrap_btn">
                    <a href="javascript:daum.Music.Talk.landingKakaoMusic('$!trackIds');" class="btn_vote link_result1">곡 리스트 전체듣기</a>
                </div>
            #end
        </div>
	</div>
#end

