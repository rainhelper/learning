<Music>
	<Songs>
		#foreach ($song in $chartSongList)
			<Song>
				<SongId><![CDATA[$!{song.songId}]]></SongId>
				<SongName><![CDATA[$!{song.title}]]></SongName>
				<AlbumId><![CDATA[$!{song.album.albumId}]]></AlbumId>
				<AlbumName><![CDATA[$!{song.album.title}]]></AlbumName>
				<AlbumImage><![CDATA[$!{song.album.mainImage.filePath}]]></AlbumImage>
				<Artists>
					#if($song.keyArtistList.size()>0)
						#foreach($artist in $song.keyArtistList)
							<Artist>
								<ArtistDetailId><![CDATA[$!{artist.artistId}]]></ArtistDetailId>
								<ArtistName>#if($artist.name)<![CDATA[$!{artist.name}]]>#else<![CDATA[아티스트없음]]>#end</ArtistName>
								<ArtistDetailUrl>#if($!{artist.artistId} && $!{artist.artistId} != '0')<![CDATA[http://music.daum.net/artist/main?artist_id=$!{artist.artistId}]]>#end</ArtistDetailUrl>
							</Artist>
						#end
					#else
						<Artist>
							<ArtistDetailId></ArtistDetailId>
							<ArtistName><![CDATA[아티스트없음]]></ArtistName>
						</Artist>
					#end
				</Artists>
				<VClipId><![CDATA[$!{song.vclipLinkId}]]></VClipId>
				<LyRicsYn><![CDATA[$!{song.lyricsYn}]]></LyRicsYn>
				<ModRank><![CDATA[$!{song.modRank}]]></ModRank>
				<MP3><![CDATA[$!{song.MP3LinkId}]]></MP3>
				<BGM><![CDATA[$!{song.BGMLinkId}]]></BGM>
				<AOD><![CDATA[$!{song.AODLinkId}]]></AOD>
				<SongDetailUrl>#if($song.songId)<![CDATA[http://music.daum.net/album/main?album_id=$!{song.album.albumId}&song_id=$!{song.songId}]]>#end</SongDetailUrl>
				<AlbumDetailUrl>#if($song.album.albumId)<![CDATA[http://music.daum.net/album/main?album_id=$!{song.album.albumId}]]>#end</AlbumDetailUrl>
				<BGMPlayUrl></BGMPlayUrl>
				<BELLYN><![CDATA[]]></BELLYN>
				<RINGYN><![CDATA[]]></RINGYN>
				<FEATARTISTS><![CDATA[$!{song.featAllArtistWithComma}]]></FEATARTISTS>
				<Rank><![CDATA[$!{song.rank}]]></Rank>
				<Freq><![CDATA[$!{song.freq}]]></Freq>
			</Song>
		#end
	</Songs>
</Music>