#set($artistList = $dto.artistResult.documentList)
$response.setContentType("text/xml")<?xml version="1.0" encoding="UTF-8"?>
#if("$!{artistList}" != "")
<Artists>
#foreach($artist in $artistList)
	<Artist>
		<ArtistId>$!{artist.artist_id}</ArtistId>
#if("$!{artist.artist_name}" != "")		<ArtistName><![CDATA[$!{artist.artist_name}]]></ArtistName>#else		<ArtistName/>#end

#if("$!{artist.thumb}" != "")		<MainImage><![CDATA[$!{artist.thumb}]]></MainImage>#else		<MainImage/>#end

#if("$!{artist.getBirthDate()}" != "")		<BirthDateTime><![CDATA[$!{artist.getBirthDate()}]]></BirthDateTime>#else		<BirthDateTime/>#end

		<ServicePageUrl><![CDATA[http://music.daum.net/artist/artist.do?artistId=$!{artist.artist_id}]]></ServicePageUrl>
#if("$!{artist.getRepresentationSong().getTitle()}" != "")		<RepresentationSongTitle><![CDATA[$!{artist.getRepresentationSong().getTitle()}]]></RepresentationSongTitle>#else		<RepresentationSongTitle/>#end

	</Artist>
#end
</Artists>
#end
