<HotSong>
	#foreach ($song in $songList)
		<Song>
			<SongId><![CDATA[$!{song.songId}]]></SongId>
			<AlbumId><![CDATA[$!{song.album.albumId}]]></AlbumId>
			<AlbumName><![CDATA[$!{song.album.title}]]></AlbumName>
			<FeatArtist><![CDATA[$!{song.featAllArtistWithComma}]]></FeatArtist>
			<SongName><![CDATA[$!{song.title}]]></SongName>
			<Artists>
				#foreach($artist in $song.keyArtistList)
					<Artist>
						<ArtistId><![CDATA[]]></ArtistId>
						<ArtistDetailId><![CDATA[$!{artist.artistId}]]></ArtistDetailId>
						<ArtistName><![CDATA[$!{artist.name}]]></ArtistName>
					</Artist>
				#end
			</Artists>
			<SongType><![CDATA[$!{song.typeName}]]></SongType>
			<AwardYn><![CDATA[$!{song.awardYn}]]></AwardYn>
			<LyRicsYn><![CDATA[$!{song.lyricsYn}]]></LyRicsYn>
			<AdultYn><![CDATA[$!{song.adultYn}]]></AdultYn>
			<VideoId><![CDATA[$!{song.vclipLinkId}]]></VideoId>
			<AOD><![CDATA[$!{song.AODLinkId}]]></AOD>
			<MP3><![CDATA[$!{song.MP3LinkId}]]></MP3>
			<BGM><![CDATA[$!{song.BGMLinkId}]]></BGM>
		</Song>
	#end
</HotSong>