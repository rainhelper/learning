    					try {
                			daum.Element.setStyle(daum.$$("body", daum.$("commentFrame").contentWindow.document.documentElement)[0],'background-color:#F7F7F4;');
    						daum.Element.setStyle(daum.$$(daum.$$(".bg_paging")[0], daum.$("commentFrame").contentWindow.document.documentElement)[0],'background:url(http://i1.daumcdn.net/img-contents/music/2011_home/sym/sym_pc/paging_comm.gif) no-repeat;');
    					} catch(e) {}
                	}
