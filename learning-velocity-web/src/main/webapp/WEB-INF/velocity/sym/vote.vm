								<li #if($StringUtils.equals("$!{rest}", "2"))class="right"#end>
									<div class="thumb">
										#foreach($image in $item.artistImages)
											#if($StringUtils.equals("$!{foreach.index}", "0"))<img src="$!{image.filePath}" width="220" height="150" alt=""/><span class="line_thumb"></span>#end
										#end
									</div>
									<div class="wrap_radio">
