		<div class="wrap_consumer">
			<a href="javascript:;" onclick="music.mobile.recommend.call('$!{artist.artistId}','artist')" class="btn_like bround"title="추천하기">
				<span class="ico_music ico_like">추천</span><em class="fc_blue" id="recommend_$!{artist.artistId}">$!artist.recommendCount</em>
			</a>
#snsLayer("f_r")
		</div>
		<div id="socialShareMoreViewContainer"></div>
	</div>
#if($albumList.size() > 0)
	<div class="section_album">
		<div class="wrap_head">
			<h4 class="tit">앨범</h4>
		</div>
		<div class="wrap_list_album">
		<ul class="list_album"> 
#foreach($album in $albumList)
			<li>
				<a href="/album/main?album_id=$!{album.albumId}" class="wrap_thumb">
					<span class="thumb">
						<img #if($isWVGA)width="141" height="141"#else width="94" height="94"#end alt="이미지정보" src="$ImageUtil.getMobileImageUrl("$!{album.mainImage.filePath}", "94x94", "ALBUM", "C")">
						<span class="bg_outline"></span>
					</span>
					<strong class="info">
						<span class="tit">
							#foreach($artist in $album.keyArtistList)#if($velocityCount!=1), #end$!{artist.name}#end
						</span>
						<span class="txt_info">#strCut( $album.title, 18)</span>
					</strong>
				</a>
			</li>
			
#end
		</ul>
		</div>
		<span class="link_more">
			<a href="/artist/album/list?artist_id=$!{artist.artistId}" class="txt_info">더보기</a> <span class="ico_music ico_arr"></span>
		</span>
	</div>
#end
#if($popularSongList.size() > 0)
	<div class="section_song">
		<div class="wrap_head">
			<h4 class="tit">$!{artist.name}의 인기차트</h4>
		</div>
		<ol class="list_info list_music">
#foreach($song in $popularSongList)
#playList()
#end
		</ol>
		<span class="link_more">
			<a href="/artist/popularSong/list?artist_id=$!{artist.artistId}" class="txt_info">더보기</a> <span class="ico_music ico_arr"></span>
		</span>
	</div>
#end
#if($vclipList.size() > 0)
	<div class="section_video">
		<div class="wrap_head">
			<h4 class="tit">뮤직비디오</h4>
		</div>
		<ul class="list_info list_thumb">
#foreach($vclip in $vclipList)
#mVideoList()
#end
		</ul>
		<span class="link_more">
			<a href="/artist/vclip/list?artist_id=$!{artist.artistId}" class="txt_info">더보기</a> <span class="ico_music ico_arr"></span>
		</span>
	</div>
#end
	<div class="cmt_comm">
		<iframe src="about:blank" name="commentFrame" id="commentFrame" width="1" height="50" marginwidth="0" marginheight="0" frameborder="0" scrolling="no" title="commentFrame"></iframe>
	</div>
<script type="text/javascript">//<![CDATA[
	daum.Event.addEvent( window, "load", function(){
		var commentFrame = $("commentFrame");
		commentFrame.width = "100%";
		commentFrame.src="#getter('GAIA_MOBILE_DOMAIN', '')/gaia/do/mobile/comment/commentList?bbsId=$!{artist.bbsId}&articleId=$!{artist.articleId}";
	});
//]]></script>
#SearchForm("only_bottom")
</div>
