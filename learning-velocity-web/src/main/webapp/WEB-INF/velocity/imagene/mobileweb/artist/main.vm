#uaCheck()
<div id="daumContent" class="cont_detail">
	<h2 class="screen_out">상세</h2>
	<div class="cont_navi">
		<h3 class="tit">아티스트 정보</h3>
#btnPrev()
	</div>
	<div class="section_info">
		<h4 class="tit">$!artist.name</h4>
		<div class="wrap_cont">
			<div class="thumb">
				<img #if($isWVGA)width="141" height="141"#else width="94" height="94"#end alt="이미지정보" src="$ImageUtil.getMobileImageUrl("$!{artist.mainImage.filePath}", "94x94", "ARTIST", "C")">
				<span class="outline"></span>
			</div>
			<div class="cont">
#if("$!artist.bandTypeName"=="")
				<dl class="info_data">
					<dt class="tit_base">활동정보</dt>
					<dd class="desc"> $!artist.creditTitle1#if("$!artist.creditTitle2"!=""), #end$!artist.creditTitle2</dd>
				</dl>
#if($StringUtils.isNotBlank("$!{artist.birthDt}"))				
				<dl class="info_data">
					<dt class="tit_base">출생</dt>
					<dd class="desc">$CheckDate.makeReleaseDate($!{artist.birthDt})</dd>
				</dl>
#end
#if($artistBandList.size() > 0)
				<dl class="info_data">
					<dt class="tit_base">소속그룹</dt>
					<dd class="desc">#foreach($artist in $artistBandList)<a href="/artist/main?artist_id=$!{artist.artistId}">$artist.name</a> #end</dd>
				</dl>
#end
#else
				<dl class="info_data">
					<dt class="tit_base">활동정보</dt>
					<dd class="desc">$!artist.bandTypeName</dd>
				</dl>
				<dl class="info_data">
					<dt class="tit_base">멤버</dt>
					<dd class="desc">
						#foreach($member in $artistCurrnetbandMemberList)#if($velocityCount!=1), #end<a href="/artist/main?artist_id=$!{member.artistId}">$!member.name</a>#end
					</dd>
				</dl>
#end
			</div>
		</div>
