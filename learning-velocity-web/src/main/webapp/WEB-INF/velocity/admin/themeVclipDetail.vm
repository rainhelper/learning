<script type="text/javascript">
	function searchVclip(){
		var f = document.search;
		f.method.value="search";
		if(f.keyword.value == "" || f.keyword.value == null){
			alert("검색어를 입력하세요");
			return;
		}
		f.submit();
	}
	
	function updateTheme(){
		var f = document.update;
		if(f.title.value == "" || f.title.value == null){
			alert("테마명을 입력하세요");
			return;
		}
		f.method.value="update";
		f.submit();
	}

	function insertVclipF(){
		var f = document.insertVclip;
		var hasChecked = false;
		for(var i = 0; i < f.vclipidList.length; i++){
			if(f.vclipidList[i].checked){
				hasChecked = true;
				break;
			}
		}
		
		if(!hasChecked){
			alert("동영상을 체크하세요");
			return;
		}
		f.method.value="insert";
		f.submit();
	}

	function insertVclipDirectF(){
		var f = document.insertDirectVclip;
		if(f.vclipidList.value == "" || f.vclipidList.value == null){
			alert("id를 입력하세요");
			return;
		}
		f.method.value="insert";
		f.submit();
	}

	function deleteVclipF(){
		var f = document.deleteVclip;
		
		var hasChecked = false;
		for(var i = 0; i < f.vclipidList.length; i++){
			if(f.vclipidList[i].checked){
				hasChecked = true;
				break;
			}
		}
		
		if(!hasChecked){
			alert("동영상을 체크하세요");
			return;
		}
		f.method.value="delete";
		f.submit();
	}
	
	function updateSvcstatus() {
		if(confirm("변경하시겠습니까?")) {
			var f = document.update;
			f.method.value = "updatestatus";
			f.submit();
		}
	}
	
	function closeF(page){
		window.close();
        if (window.opener && !window.opener.closed) {
        	window.opener.location.href ="/admin/themeVclip?page_no="+page;
        } 
	}
</script>
<table class="list">
	<thead>
		<tr>
			<th width="200">테마명</th>
			<th>서비스상태</th>
            <th></th>
        </tr>
	</thead>
	<tbody>
	<form name="update" action="/admin/themeVclipDetail" method="post">
		<input type="hidden" name="method">
		<input type="hidden" name="vclipthemeid" value="$themeVclip.vclipthemeid">
		<tr>
            <td width="200"><input type="text" width="200" name="title" value="$themeVclip.title"></input></td>
			<td>
				<select name="svcstatus" onchange="javascript: updateSvcstatus()">
					<option value="0" #if ("0" == "$!themeVclip.svcstatus")selected#end>준비</option>
					<option value="2" #if ("2" == "$!themeVclip.svcstatus")selected#end>서비스</option>
					<option value="7" #if ("7" == "$!themeVclip.svcstatus")selected#end>삭제</option>
				</select>
			</td>
			<td>
				<input type="button" value="테마명수정" onClick="updateTheme()">
            </td>
        </tr>
    </form>
	</tbody>
</table>
<table class="list">
	<tr class="header">
        <td colspan="2">연결동영상</td>
        <td ><form name="search" action="/admin/themeVclipDetail" method="post">
				<select name="searchType">
                    <option value="TITLE">제목</option>
                    <option value="SONGTITLE">곡명</option>
                    <option value="ALBUMTITLE">앨범명</option>
                </select>
				<input type="hidden" name="vclipthemeid" value="$command.vclipthemeid"/>
				<input type="hidden" name="method"/><input type="text" name="keyword"/>
				<input type="button" value="검색하기" onClick="searchVclip()"/>
			</form>
		</td>
    </tr>
	<tr>
    </tr>
	<form name="deleteVclip" action="/admin/themeVclipDetail" method="post">
		<input type="hidden" name="method">
		<input type="hidden" name="vclipthemeid" value="$themeVclip.vclipthemeid">
	#foreach($item in $vclipList)
        <tr>
            <td><input type="checkbox" name="vclipidList" value="$item.vclipId"></td>
			<td>$item.vclipId
			</td>
            <td><a href="http://music.daum.net/artist/vclip/detail?vclip_id=$item.vclipId" target="_balnk">$item.title</a>
            </td>
			<td>#if ($item.svcstatus == "1") 예약 #elseif ($item.svcstatus == "0") 준비 #elseif ($item.svcstatus == "2") 서비스 #elseif ($item.svcstatus == "9") 삭제 #end</td>
		</tr>
	#end
    <tr>
		<td colspan="3">
			<input type="button" value="삭제하기" onClick="deleteVclipF()">
        </td>
	</tr>
    </form>
    <tr>
		<td colspan="3">
