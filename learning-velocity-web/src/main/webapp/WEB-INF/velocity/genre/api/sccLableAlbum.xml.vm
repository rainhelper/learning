<?xml version="1.0" encoding="UTF-8"?>
<datalist>
<count><![CDATA[$allLableList.size()]]></count>	
#if(${allLableList.size()} > 0)	
	#foreach($item in $allLableList)
	<data>
		#set($lable = $lableAlbumMap.get($item.lableId))
		#set($lableSearchKeywordList = $lableSearchKeywordMap.get($item.lableId))
      	<lastModified><![CDATA[$lable.moddttm]]></lastModified>
      	<regDate><![CDATA[$lable.regdttm]]></regDate>
      	<dataKey><![CDATA[$lable.lableId]]></dataKey>
			<field>                                    
			  <id>text01</id>
			  <item>
                <link><![CDATA[]]></link>
                <content><![CDATA[$lable.lableName]]></content>
              </item>
			</field>                                   
			<field>                                    
			  <id>text02</id>
			  <item>
                <link><![CDATA[]]></link>
                <content><![CDATA[]]></content>
              </item>
			</field>                                   
			<field>                                    
			  <id>text03</id>
			  #foreach($keyword in $lableSearchKeywordList)
				<item>
                    <link><![CDATA[]]></link>
                    <content><![CDATA[$keyword]]></content>
                </item>
			  #end
			</field> 
			<field>                                    
			  <id>text04</id> 
				<item>
                    <link><![CDATA[]]></link>
                    <content><![CDATA[#if("$lable.domesticYn" == "Y")국내#else해외#end]]></content>
                </item>
			</field>  
			<field>                                    
			  <id>text05</id>       
			  #if("$lable.domesticYn"=="Y")
				#foreach($domLable in $domesticLableList)
    			  <item>
				    <link><![CDATA[]]></link>              
				    <content><![CDATA[$domLable.lableName]]></content>  
    			  </item>                                  
				#end
			#else
				#foreach($forLable in $foreignLableList)
    			  <item>
				    <link><![CDATA[]]></link>              
				    <content><![CDATA[$forLable.lableName]]></content>  
    			  </item>                                  
				#end
			  #end
			  
			</field>
			#foreach($id in [0 .. 6])
				#set($filedId = $id+6)
				#if($filedId < 10)
					#set($filedId = "0"+$filedId)
				#end
			<field>                                    
			  <id>text$!{filedId}</id>
			<item>
              <link><![CDATA[]]></link>
			  #if($lable.albumListInLable.size() > $id)
				#set($album = $lable.albumListInLable.get($id))
              <content><![CDATA[$ImageUtil.getImageUrl("$album.mainImage.filePath", "110x110", "ALBUM", "C")]]></content>
			  #else
              <content><![CDATA[]]></content>
			  #end
            </item>
			</field>  
			#end
			<field>                                    
			  <id>text13</id>                          
				#foreach($id in [0 .. 6])
					#if($lable.albumListInLable.size() > $id)
						#set($album = $lable.albumListInLable.get($id))
    					#set($masterSong = "=")
                		#if($StringUtils.isNotBlank("$!{album.masterSongList}") && $!{album.masterSongList.size()} > 0 )
                			#set($masterSong = $ListTool.get($!{album.masterSongList}, 0))
                		#else
                			#if($!{album.songList.size()}>0)#set($masterSong = $ListTool.get($!{album.songList}, 0))#end
                		#end
    				  <item>
                        <link><![CDATA[]]></link>
                        <content><![CDATA[#if(($StringUtils.isNotBlank("$!{masterSong.AODLinkId}") && !$StringUtils.equals("$!{masterSong.AODLinkId}", "0")))Y#{else}N#{end}]]></content>
                      </item>
					#else
    				  <item>
                        <link><![CDATA[]]></link>
                        <content><![CDATA[]]></content>
                      </item>
					#end
				#end
			</field>  
			<field> 
			  <id>text14</id>                          
				  <item>                                   
				    <link><![CDATA[http://music.daum.net/genre/lablealbum/list?lable_id=$lable.lableId]]></link>              
				    <content><![CDATA[$lable.totCountInLable]]></content>  
				  </item>
			</field> 
			<field>                                    
			  <id>text15</id>                          
				#foreach($id in [0 .. 6])
					#if($lable.albumListInLable.size() > $id)
						#set($album = $lable.albumListInLable.get($id))
    				  <item>
                        <link><![CDATA[http://music.daum.net/album/main?album_id=$album.albumId]]></link>
                        <content><![CDATA[$album.title]]></content>
                      </item>
					#else
    				  <item>
                        <link><![CDATA[]]></link>
                        <content><![CDATA[]]></content>
                      </item>
					#end
				#end
			</field>  
			<field>                                    
			  <id>text16</id>                          
				#foreach($id in [0 .. 6])
					#if($lable.albumListInLable.size() > $id)
						#set($album = $lable.albumListInLable.get($id))
						#if($album.keyArtistList.size() > 0)
							#set($artist = $album.keyArtistList.get(0))
            				  <item>
                                <link><![CDATA[http://music.daum.net/artist/main?artist_id=$artist.artistId]]></link>
                                <content><![CDATA[$artist.name]]></content>
                              </item>
						#end
					#else
    				  <item>
                        <link><![CDATA[]]></link>
                        <content><![CDATA[]]></content>
                      </item>
				   #end
				#end
			</field>  
			<field>                                    
			  <id>text17</id>                          
				#foreach($id in [0 .. 6])
					#if($lable.albumListInLable.size() > $id)
						#set($album = $lable.albumListInLable.get($id))
    				  <item>
                        <link><![CDATA[]]></link>
                        <content><![CDATA[$album.releaseDt]]></content>
                      </item>
					#else
    				  <item>
                        <link><![CDATA[]]></link>
                        <content><![CDATA[]]></content>
                      </item>
					#end
				#end
			</field>  
		</data>			
	#end
#end	
</datalist>
