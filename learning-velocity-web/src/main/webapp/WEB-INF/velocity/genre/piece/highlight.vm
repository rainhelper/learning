#if($StringUtils.isNotBlank("$!{genre.genreCode}"))
<div class="wrap_list_keyword">
	<ul id="subgenreName" class="list_keyword">
		<li >
			<a href="/genre/genrealbum/list?genre_group_cd=$!{genre.genreCode}"><span>종합</span></a>
		</li>
		#set($subliCount = 0)
		#foreach($subgenre in $genre.genreList)
		#set($subliCount = $subliCount + 1)
		<li code="$!{subgenre.genreCode}">		
			#if($StringUtils.equals("$!{subgenre.recommendGenreYn}","Y"))<strong>#end
			##<a href="/genre/genrealbum/list?genre_group_cd=$!{genre.genreCode}&genre_cd=$!{subgenre.genreCode}" onclick="daum.Music.subgenreDetail('$!{genre.genreCode}','$!{command.genre_cd}','$!{subgenre.genreCode}');return false;"></a>
			<a href="/genre/genrealbum/list?genre_group_cd=$!{genre.genreCode}&genre_cd=$!{subgenre.genreCode}">
				<span>$subgenre.genreName</span></a>
			#if($StringUtils.equals("$!{subgenre.recommendGenreYn}","Y"))</strong>#end
		</li>
		#end
		#if($subliCount < 7)
		#foreach($i in [$subliCount..7])
			<li><span class="subli_blank"></span></li>
		#end
		#end
	</ul>
	<div class="cl_b"></div>

	<span class="bg_box bg_top"></span>
	<span class="bg_box bg_btm"></span>
</div>
<script type="text/javascript">
new daum.Music.Genre("tabgenreName","subgenreName", {groupGenreName:"$!Escape.url($!{genre.genreName})",subGenre:"$!Escape.url($!{command.genre_cd})"});
</script>
#else
<div class="wrap_list_keyword wrap_list_label"> 	
	<ul class="tab_label">
		<li id="domesticTab" class="on">
			<a class="domestic ir_pm" href="#">국내</a>
		</li>
		<li id="foreignTab" class="">
			<a class="foreign ir_pm" href="#">해외</a>
		</li>
	</ul>
	<ul id="domesticList" class="list_keyword" style="display:block">
		#foreach($lable in $domesticLableList)
		<li id="$!{lable.lableId}">
			<a href="/genre/lablealbum/list?lable_id=$!{lable.lableId}"><span>$lable.lableName</span></a>
		</li>
		#end
	</ul>
	<ul id="foreignList" class="list_keyword" style="display:none">
		#foreach($lable in $foreignLableList)
		<li id="$!{lable.lableId}">
			<a href="/genre/lablealbum/list?lable_id=$!{lable.lableId}" ><span>$lable.lableName</span></a>
		</li>
		#end
	</ul>
	<div class="cl_b"></div>
	<span class="bg_box bg_top"></span>
	<span class="bg_box bg_btm"></span>
	#if($domesticLableList && $domesticLableList.size() > 19)
		<div id="lable_add_border" class="add_border"></div>
	#end
</div>
<script type="text/javascript">
new daum.Music.Genre("tabgenreName","subgenreName",{chooselabel:"$!{chooseLable.lableId}",isGenre:false,labelTab1:"domesticTab", labelTab2:"foreignTab",list1:"domesticList",list2:"foreignList",addborder:"lable_add_border"});
</script>
#end



