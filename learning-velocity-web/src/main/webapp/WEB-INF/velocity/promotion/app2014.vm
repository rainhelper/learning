<script type="text/javascript">

	function leadingZeros(n, digits){
	var zero = '';
	n = n.toString();
	if (n.length < digits) {
	  for (var i=0; i < digits - n.length; i++)
		zero += '0';
	}
	return zero + n;
}

function setCurrentTime(currentTime){
	var s = currentTime;
	var d = new Date();

	if(s == "" || s == undefined) s = leadingZeros(d.getHours(), 2)+ leadingZeros(d.getMinutes(), 2)+ leadingZeros(d.getSeconds(), 2);


	
	var hour1 = s.substr(0,1);
	var hour2 = s.substr(1,1);
	var minutes1 = s.substr(2,1);
	var minutes2 = s.substr(3,1);
	var seconds1 = s.substr(4,1);
	var seconds2 = s.substr(5,1);
	



	var dateString = "<div class=\"num_time\">"
				+	"<div class=\"img_num num_type"+hour1+"\"></div>"
				+	"<div class=\"img_num num_type"+hour2+"\"></div>"
				+"</div>"
				+"<div class=\"num_time\">"
				+	"<div class=\"img_num num_type"+minutes1+"\"></div>"
				+	"<div class=\"img_num num_type"+minutes2+"\"></div>"
				+"</div>"
				+"<div class=\"num_time\">"
				+	"<div class=\"img_num num_type"+seconds1+"\"></div>"
				+	"<div class=\"img_num num_type"+seconds2+"\"></div>"
				+"</div>"
			+"</div>";
	document.getElementById("time_block").innerHTML = dateString;
}

setCurrentTime();

setInterval(setCurrentTime, 1000);
</script>

